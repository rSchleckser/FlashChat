{
    "particles": {
        "number": {
            "value": 500
        },
        "color": {
            "value": "#fff"
        },
        "shape": {
            "type": "star",
            "stroke": {
                "width": 4,
                "color": "#000"

            },
            "image": {
                "src": "Shuyamobile.jpg"
            }
        },
        "opacity": {
            "value": 0.5,
            "random": true,
            "anim": {
                "enable": true,
                "speed": 10
            }
        },

        "size": {
            "value": 3
        },
        "move": {
            "enable": true,
            "speed": 10
        },
        "line_linked": {
            "enable": true,
            "distance": 80,
            "color": "#ffffff",
            "opacity": 0,
            "width": 0
        }

    },


    "interactivity": {
        "detect_on": "window",
        "events": {
            "onhover": {
                "enable": true,
                "mode": "grab"
            },
            "onclick": {
                "enable": false,
                "mode": "push"
            },
            "resize": true
        },
        "modes": {
            "grab": {
                "distance": 400,
                "line_linked": {
                    "opacity": 1
                }
            }
        }
    }
}